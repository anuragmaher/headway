        "sentences": [
          {
            "start": 2687,
            "end": 3597,
            "text": "Hey, yes, I can."
          },
          {
            "start": 3677,
            "end": 4037,
            "text": "How are you?"
          }
        ]
      },
      {
        "speakerId": "8255523944008136035",

        "speakerId": "8900740095488669230",
        "topic": null,
        "sentences": [
          {
            "start": 704262,
            "end": 710602,
            "text": "Departments handle different sections of the master list and then they email it to them manually."
          },
          {
            "start": 713252,
            "end": 713852,
            "text": "Got it."
          },
          {
            "start": 714552,
            "end": 714992,
            "text": "Yeah."
          },
          {
            "start": 715652,
            "end": 716092,
            "text": "So."
